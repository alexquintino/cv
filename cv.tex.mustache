{{=<% %>=}}
%!TEX TS-program = xelatex
\documentclass[print]{amsquintino-cv}

\begin{document}
<%#bio%>
\header{<%first_name%>}{<%last_name%>}{}

% In the aside, each new line forces a line break
\begin{aside}
  \section{about}
      <%#private_address%>
        <%.%>
      <%/private_address%>
      <%private_phonenumber %> {\iconfont\symbol{"F095}}
      {\small \href{mailto:<%email%>}{<%email%> {\iconfont\symbol{"F0E0}}}}
      \href{https://<%website%>}{{\small <%website%>} {\iconfont\symbol{"F015}}}
      \href{https://<%github%>}{{\small <%github%>} {\iconfont\symbol{"F09B}}}
      \href{https://twitter.com/<%twitter%>}{{\small <%twitter%>} {\iconfont\symbol{"F099}}}
      {\small <%private_skype%>} {\iconfont\symbol{"F17E}}
  \section{interests}
    <%#interests%>
      <%.%>
    <%/interests%>
  \section{languages}
    <%#languages%>
      <%.%>
    <%/languages%>
\end{aside}
<%/bio%>

\section{experience}
\begin{experience}
  <%#experience%>
    \experienceentry
      {from <%start_date%>}
      {to <%end_date%>}
      {<%title%> <%#company%> @ <%.%> <%/company%>}
      {<%location%>}
      <%#responsibilities%>
        {<%main%>}
        {<%#items.length%>
          \begin{itemize}[leftmargin=*]
          \itemsep 0.3em
            <%#items%>
              \item <%& .%>
            <%/items%>
          \end{itemize}
        <%/items.length%>}
      <%/responsibilities%>
  <%/experience%>
\end{experience}

\section{side projects}
<%#side_projects%>
{\large\textbf{<% title %>}} \\
{\footnotesize\addfontfeature{Color=lightgray} \href{https://<%url%>}{<%url%>}}\\
<% description %> \\[\parsep]
<%/side_projects%>

\section{education}
\begin{education}
  <%#education%>
  \educationentry
    {from <%start_date%>}
    {to <%end_date%>}
    {<%type%> in <%name%>}
    {<%institution%>, <%location%>}
  <%/education%>
\end{education}

\end{document}
